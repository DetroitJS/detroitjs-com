{
  "name": "next-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "bulma": "^0.3.1",
    "momentjs": "^2.0.0",
    "next": "latest",
    "node-sass": "^4.3.0"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:sass\" \"npm run dev:next\"",
    "dev:sass": "node-sass -w ./assets/sass/app.scss ./static/app.css",
    "dev:next": "./node_modules/.bin/next",
    "build:sass": "node-sass ./assets/sass/app.scss ./static/app.css",
    "build:next": "./node_modules/.bin/next build",
    "build": "npm run build:sass && npm run build:next",
    "start": "./node_modules/.bin/next start"
  },
  "devDependencies": {
    "concurrently": "^3.1.0"
  }
}
